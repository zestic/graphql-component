{
  "name": "iampersistent/graphql-component",
  "type": "library",
  "description": "GraqhQL component",
  "keywords": ["GraqhQL", "Zend Expressive"],
  "license": "MIT",
  "require": {
    "firebase/php-jwt": "^5.2.0",
    "php": ">=7.4",
    "psr/container": "^1.0",
    "psr/log": "^1.0",
    "symfony/messenger": "^4.0 || ^5.0",
    "laminas/laminas-diactoros": "^1.7"
  },
  "require-dev": {
    "codeception/aspect-mock": "dev-master",
    "codeception/codeception": "^2.3.8"
  },
  "repositories": {
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "IamPersistent\\GraphQL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixture\\": "tests/_support/Fixture",
      "Tests\\Functional\\": "tests/functional",
      "Tests\\Unit\\": "tests/unit"
    }
  }
}
