{
  "name": "iampersistent/graphql-component",
  "type": "library",
  "description": "GraqhQL component",
  "keywords": ["GraphQl", "Zend Expressive"],
  "license": "MIT",
  "require": {
    "firebase/php-jwt": "dev-master",
    "php": ">=7.1",
    "psr/container": "^1.0",
    "psr/log": "^1.0",
    "zendframework/zend-diactoros": "^1.7",
    "zestic/common": "*"
  },
  "require-dev": {
    "codeception/aspect-mock": "dev-master",
    "codeception/codeception": "^2.3.8"
  },
  "repositories": {
    "zestic/common": {
      "type": "vcs",
      "url": "git@github.com:zestic/common.git"
    }
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "IamPersistent\\GraphQL\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixture\\": "tests/_support/Fixture",
      "Tests\\Functional\\": "tests/functional",
      "Tests\\Unit\\": "tests/unit"
    }
  }
}
